        <a
          class="h-8 w-20 flex justify-center items-center bg-my-n-yellow text-black shadow-black-y transition-all duration-300 ease-in-out hover:shadow-none"
          href="/signup"
          >Sign up</a
        >
        <% } %>
      </div>
    </div>

